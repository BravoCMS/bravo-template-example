<{*if $tag_attributes.type === 'schedule'}>    <div class="block_grafic">        <p class="job-t">{company_timetable}</p>        <{if $contacts_data.contact.24h_work === '1'}>            <p class="job-tt">                {schedule_24h}            </p>        <{else}>            <{foreach $contacts_data.contact.schedule as $daySchedule}>                <p class="job-tt">                    <{if $daySchedule.day_from === $daySchedule.day_to}>                        <{$daySchedule.day_to}>:                    <{else}>                        <{$daySchedule.day_from}>-<{$daySchedule.day_to}>:                    <{/if}>                    <span>                        <{if $daySchedule.holiday === '1'}>                            {holiday}                        <{else}>                            <{$daySchedule.time_from_h}><sub>:<{$daySchedule.time_from_m}></sub>-<{$daySchedule.time_to_h}><sub>:<{$daySchedule.time_to_m}></sub>                        <{/if}>                    </span>                </p>            <{/foreach}>        <{/if}>    </div><{elseif $tag_attributes.type === 'map'}>    <{if $contacts_data.contact.map === 'google'}>        <script type="text/javascript" charset="utf-8">            jQuery(function ($) {                if (google) {                    var $element = $("#map-<{$contacts_tag_instance_id}>");                    var zoom = $element.data("zoom");                    var lat = $element.data("latitude");                    var lng = $element.data("longitude");                    if (lat && lng) {                        var myLatlng = new google.maps.LatLng(lat, lng);                    } else {                        var myLatlng = new google.maps.LatLng(52.520591, 13.407111);                    }                    var myOptions = {                        zoom: zoom ? zoom : 17,                        center: myLatlng,                        mapTypeId: google.maps.MapTypeId.ROADMAP                    }                    var map = new google.maps.Map($element.get(0), myOptions);                    var marker = new google.maps.Marker({                        position: myLatlng,                        map: map                    });                }            });        </script>    <{elseif $contacts_data.contact.map === 'yandex'}>        <script type="text/javascript" charset="utf-8">            jQuery(function ($) {                ymaps.ready(function () {                    var $element = $("#map-<{$contacts_tag_instance_id}>");                    var zoom = $element.data("zoom");                    var lat = $element.data("latitude");                    var lng = $element.data("longitude");                    var map = new ymaps.Map($element.get(0), {                        center: [lat, lng],                        zoom: zoom ? zoom : 17                    });                    var marker = new ymaps.Placemark([lat, lng]);                    map.geoObjects.add(marker);                });            });        </script>    <{/if}>    <div class="contacts-tag-map-element" id="map-<{$contacts_tag_instance_id}>" data-zoom="<{$tag_attributes.zoom|escape}>" data-latitude="<{$contacts_data.contact.latitude|escape}>" data-longitude="<{$contacts_data.contact.longitude|escape}>"></div><{elseif $tag_attributes.type === 'page'}>    <{if $contacts_data.page}>        <div class="block_contacts_page">            <a href="<{$contacts_data.page.href}>" title="<{$contacts_data.page.title|escape}>">                {all_contacts}            </a>        </div>    <{/if}><{elseif $tag_attributes.type === 'catalog_phone'}>    <{if $contacts_data.catalog_phones|count}>        <p>            {questions_and_orders_by_phone}        </p>        <{foreach $contacts_data.catalog_phones as $phone}>            <div class="contact-phone">                <a href="tel:<{$phone.number|strip_tags|trim|escape}>">                    <{$phone.number}>                </a>            </div>        <{/foreach}>    <{/if}><{else}>    <{if $tag_attributes.address_no || $tag_attributes.address_id}>        <{foreach $contacts_data.contact.phones as $phone}>            <{if !$tag_attributes.value || $phone@iteration < $tag_attributes.value}>                <p class="phone_t">                    <a href="tel:<{$phone.number|strip_tags|trim|escape}>">                        <{$phone.number}>                    </a>                    <{if $phone.fax === 'Y'}>                        <span class="phone_t_fax">                            {phone_fax}                        </span>                    <{/if}>                    <{if $phone.mobile === 'Y'}>                        <span class="phone_t_mobile">                            {phone_mobile}                        </span>                    <{/if}>                </p>            <{/if}>        <{/foreach}>    <{else}>        <{foreach $contacts_data.phones as $phone}>            <{if !$tag_attributes.value || $phone@iteration < $tag_attributes.value}>                <p class="phone_t">                    <a href="tel:<{$phone.number|strip_tags|trim|escape}>">                        <{$phone.number}>                    </a>                    <{if $phone.fax === 'Y'}>                        <span class="phone_t_fax">                            {phone_fax}                        </span>                    <{/if}>                    <{if $phone.mobile === 'Y'}>                        <span class="phone_t_mobile">                            {phone_mobile}                        </span>                    <{/if}>                </p>            <{/if}>        <{/foreach}>    <{/if}><{/if*}>